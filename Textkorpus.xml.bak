     b3:ana="N"
     c3:ana="N"
     b4:ana="N"
     c4:ana="N"
     b5:ana="N"
     c5:ana="N"
     d:ana="N">hätten</seg>
</p>
<p>
<seg a:ana="N"
     b1:ana="N"
     c1:ana="N"
     b2:ana="N"
     c2:ana="N"
     b3:ana="N"
